<?xml version="1.0" encoding="utf-8"?>
<Container version="2">
    <Name>Romarr</Name>
    <Overview>An application to handle rom management</Overview>
    <Registry>https://registry.hub.docker.com/u/jholsclaw79/romarr/</Registry>
    <Repository>jholsclaw79/romarr</Repository>
    <Privileged>false</Privileged>
    <Network>bridge</Network>
    <WebUI>http://[IP]:[PORT:9487]</WebUI>
    <Config
        Type="Port"
        Name="Web UI"
        Target="9487"
        Default="9487"
        Mode="tcp"
        Description="Web UI Port"
        Display="always"
        Required="true">9487</Config>
    <Config
        Type="Path"
        Name="LaunchBox Installation Folder"
        Target="/launchBox"
        Default=""
        Mode="rw"
        Description="LaunchBox Installation Folder"
        Display="always"
        Required="true"></Config>
    
</Container>